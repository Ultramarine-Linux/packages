[org.gnome.desktop.background:Budgie]
picture-uri='file:///usr/share/backgrounds/ultramarine-linux/40/lost-light.png'
picture-uri-dark='file:///usr/share/backgrounds/ultramarine-linux/40/lost-dark.png'

[org.gnome.desktop.wm.preferences:Budgie]
button-layout = 'appmenu:minimize,maximize,close'
titlebar-font='Inter Bold 11'

[org.gnome.desktop.screensaver:Budgie]
picture-uri='file:///usr/share/backgrounds/ultramarine-linux/40/lost-dark.png'

[org.gnome.gedit.preferences.editor:Budgie]
scheme='textmate'

[org.gnome.desktop.interface:Budgie]
gtk-theme='Fluent-round-Dark'
icon-theme='Fluent-dark'
color-scheme='prefer-dark'
font-name='Inter 11'
document-font-name='Inter 11'

[org.ubuntubudgie.windowshuffler:Budgie]
runshuffler=true

[org.gnome.nm-applet:Budgie]
show-applet=false

[com.solus-project.budgie-panel:Budgie]
builtin-theme=false
dark-theme=true
migration-level=1
notification-position='BUDGIE_NOTIFICATION_POSITION_BOTTOM_RIGHT'
layout='ultramarine-marina'

[org.buddiesofbudgie.budgie-desktop-view:Budgie]
show=true
click-policy='double'
show-active-mounts=true

[org.ubuntubudgie.budgie-wpreviews:Budgie]
enable-previews=true

[com.solus-project.budgie-wm:Budgie]
button-layout='appmenu:minimize,maximize,close'
button-style='traditional'
caffeine-mode=false
center-windows=false

